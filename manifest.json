{
    "manifest_version": 3,
    "name": "Slime Game",
    "version": "1.0",
    "description": "Relax and have a good slime!",
    "action": {
        "default_icon": "images/Slime_Artwork.png"
      },
      "icons": {
        "16": "images/Slime_Artwork.png"
      },
      "content_scripts": [
        {
          "js": ["scripts/main.js"],
          "matches": [
            "<all_urls>"
          ]
        }
      ]
}